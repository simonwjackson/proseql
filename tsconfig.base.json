{
  "compilerOptions": {
    "target": "ES2022",
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "composite": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@proseql/core": ["./packages/core/src/index.ts"],
      "@proseql/core/*": ["./packages/core/src/*"]
    }
  }
}
