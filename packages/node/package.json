{
  "name": "@proseql/node",
  "version": "0.1.0",
  "description": "Node.js file system adapter for ProseQL, re-exports core plus NodeStorageLayer",
  "type": "module",
  "scripts": {
    "build": "bunx tsc --build",
    "clean": "rm -rf dist *.tsbuildinfo",
    "prepublishOnly": "bun run build && bun test"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/simonwjackson/proseql.git",
    "directory": "packages/node"
  },
  "keywords": [
    "database",
    "typescript",
    "effect",
    "yaml",
    "json",
    "plain-text",
    "type-safe",
    "in-memory",
    "node",
    "filesystem"
  ],
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "dependencies": {
    "@proseql/core": "workspace:*"
  }
}
